<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.blizzard.com/wow/ui/
    ..\..\FrameXML\UI.xsd">
  <Script File="lua/cli.lua"/>
  <Script File="lua/main.lua"/>
    
  <Frame name="TestAddon3_MainFrame"
         parent="UIParent"
         hidden="false"
         enableMouse="false"
         movable="false">

    <Size x="63" y="40"/>
    <Anchors>
      <Anchor point="TOP">
        <Offset x="0" y="-90"/>
      </Anchor>
    </Anchors>

    <Frames>
    </Frames>

    <Layers>
      <Layer level="BACKGROUND" setAllPoints="true">
        <Texture name="foo">
          <Color r="0" g="0" b="0" a="1"/>
        </Texture>
      </Layer>
      <Layer>
        <FontString name="$parent_xCoorNum"
                    inherits="SystemFont_Small"
                    text="0.0"
                    justifyV="TOP"
                    justifyH="RIGHT">

          <Size x="0" y="10"/>
          <Anchors>
            <Anchor point="TOPLEFT">
              <Offset x="5" y="-5"/>
            </Anchor>
          </Anchors>
          <Color r="1" g="1" b="1" a="1"/>
        </FontString>

      </Layer>
    </Layers>


    <Scripts>
      <OnLoad function="onload"/>
      <OnUpdate function="onupdate"/>
    </Scripts>

  </Frame>
</Ui>
